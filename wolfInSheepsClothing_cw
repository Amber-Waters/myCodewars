function warnTheSheep(queue) {
  let lineup = queue.reverse()
  for (i=0; i < queue.length; i++){
    if (lineup[0] === "wolf"){
      return "Pls go away and stop eating my sheep"
    }else{
      return `Oi! Sheep number ${lineup.indexOf('wolf')}! You are about to be eaten by a wolf!`
    }
    }
  }

